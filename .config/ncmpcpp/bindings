def_key "j"
  scroll_down

def_key "J"
  page_down

def_key "k"
  scroll_up

def_key "K"
  page_up

def_key "l"
  next_column

def_key "l"
  slave_screen

def_key "h"
  previous_column

def_key "h"
  master_screen

def_key "g"
  move_home

def_key "G"
  move_end

def_key "n"
  next_found_item

def_key "N"
  previous_found_item

def_key "d"
  delete_playlist_items

def_key "l"
  show_lyrics
